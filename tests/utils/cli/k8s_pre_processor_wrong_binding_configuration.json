{
  "verbose": true,
  "stream": true,
  "input": {
      "one_puller": {
          "model": "HWPCReport",
          "type": "mongodb",
          "uri": "one_uri",
          "db": "my_db",
          "collection": "my_collection"
      }
  },
  "output": {
      "one_pusher": {
          "type": "mongodb",
          "model": "PowerReport",
          "uri": "second_uri",
          "db": "my_db_result",
          "collection": "my_collection_result"
      }
  },
  "pre-processor": {
      "my_processor": {
          "type": "k8s",
          "k8s-api-mode": "Manual",
          "time-interval": 70,
          "timeout-query": 80,
          "puller": "one_pusher"
      }
  }
}