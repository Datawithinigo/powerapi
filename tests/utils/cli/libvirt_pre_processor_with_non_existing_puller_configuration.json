      "one_puller": {
          "model": "HWPCReport",
          "type": "mongodb",
          "uri": "one_uri",
          "db": "my_db",
          "collection": "my_collection"
      }
  },
  "output": {
      "one_pusher": {
          "type": "mongodb",
