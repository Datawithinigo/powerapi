{
  "verbose": true,
  "stream": true,
  "input": {
      "one_puller": {
          "model": "HWPCReport",
          "type": "mongodb",
          "uri": "one_uri",
          "db": "my_db",
          "collection": "my_collection"
      }
  },
  "output": {
      "one_pusher": {
          "type": "mongodb",
          "model": "PowerReport",
          "uri": "second_uri",
          "db": "my_db_result",
          "collection": "my_collection_result"
      }
  },
  "processor": {
      "my_processor": {
          "type": "k8s",
          "ks8_api_mode": "Manual",
          "time_interval": 50,
          "timeout_query": 60
      }
  },
  "binding": {
      "b1": {
        "from": "input.non_existent_puller",
        "to": "processor.my_processor"
    }
  }
}